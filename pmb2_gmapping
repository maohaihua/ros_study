sed -i "s/pal/nuc/g" `grep -rl pal ./`

sed -i "s/Pal/Nuc/g" `grep -rl Pal ./`

sed -i "s/PAL/NUC/g" `grep -rl PAL ./`

sed -i "s/pmb2/nucedog/g" `grep -rl pmb2 ./`

sed -i "s/Pmb2/nucedog/g" `grep -rl Pmb2 ./`

sed -i "s/PMB2/NUCEDOG/g" `grep -rl PMB2 ./`
find . -name '*' |rename 's/pmb2/nucedog/'

